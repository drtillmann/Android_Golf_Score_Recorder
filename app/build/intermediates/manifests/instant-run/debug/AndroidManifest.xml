<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.example.events"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15" />

    <application
        android:debuggable="true"
        android:icon="@drawable/golf_icon"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.example.events.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.example.events.displayData"
            android:label="Display Data" >
        </activity>
        <activity
            android:name="org.example.events.enterData"
            android:label="Enter Data" >
        </activity>
        <activity
            android:name="org.example.events.clubTracker"
            android:label="Club Tracker" >
        </activity>
        <activity
            android:name="org.example.events.Options"
            android:label="@string/options_label" >
        </activity>
        <activity
            android:name="org.example.events.Info"
            android:label="@string/info_label"
            android:theme="@android:style/Theme.Dialog" >
        </activity>

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="org.example.events.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>